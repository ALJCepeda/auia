 {
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "ban-types": false,
    "interface-name": [false],
    "no-console": [false],
    "quotemark": [true, "single"],
    "typedef-whitespace": [false],
    "trailing-comma": false,
    "max-line-length": false,
    "max-classes-per-file": false,
    "whitespace": [true, "check-decl", "check-module", "check-operator", "check-preblock", "check-rest-spread", "check-separator", "check-type-operator", "check-typecast"],
    "variable-name": false,
    "no-shadowed-variable": false,
    "no-empty": false
  },
  "rulesDirectory": []
}
